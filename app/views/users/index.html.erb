<div class="container">            
  <table class="table">
  <thead>
    <tr>
      <th>email</th>
      <th>role</th>
      
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td><%= button_to "Destroy", admin_destroy_user_path(user), method: :delete, form: { data: { 'turbo-confirm': 'Are you sure?' } } %>
        </td>
      </tr>
    <%end%>
  </tbody>
